<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nekonote" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ひらがなのおべんきょう</name>
    <description>
        ひらがなののおべんきょう
    </description>
    <author email="info@takaki.co.uk" href="http://takaki.co.uk">
        T. Himeno
    </author>
    <content src="index.html" />
    <preference name="Orientation" value="landscape" />
    <preference name="DisallowOverscroll" value="true" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-media" spec="~2.4.0" />
    <plugin name="cordova-plugin-device" spec="~1.1.3" />
</widget>
